 '../conf'\r\n\r\nclass TextSlide {\r\n\tconstructor(slideData, app) {\r\n\t\tthis.id = slideData.id\r\n\t\t// confs\r\n\t\tconst { colors, textSlider } = Conf\r\n\t\tconst textSize = textSlider.textSize\r\n\t\tconst { stage, renderer } = app\r\n\t\tconst { section } = slideData\r\n\t\tconst bgColor = section === 'formation' ? colors.bluedark : colors.bordeau\r\n\r\n\t\tconst text = new PIXI.Text(slideData.text)\r\n\t\ttext.style = {\r\n\t\t\tfill: colors.beige,\r\n\t\t\tfontSize: textSize,\r\n\t\t\talign: 'center',\r\n\t\t\tfontFamily: 'SilkSerif-Regular',\r\n\t\t}\r\n\t\t// Slide Container\r\n\t\tconst slideContainer = new PIXI.Container()\r\n\t\t// bg\r\n\t\tthis.bg = new PIXI.Graphics()\r\n\t\tthis.bg.beginFill(bgColor)\r\n\t\tthis.bg.drawRect(0, 0, window.innerWidth, window.innerHeight)\r\n\t\tthis.bg.endFill()\r\n\t\tthis.bg.x = 0\r\n\t\tthis.bg.y = 0\r\n\t\tslideContainer.addChild(this.bg)\r\n\t\tslideContainer.x = 0\r\n\t\tslideContainer.y = 0\r\n\r\n\t\t// Création d'un groupe avec le Text et son calque mask\r\n\t\tthis.textGroup = new PIXI.Container()\r\n\t\t/**\r\n\t\t * RENDER TEXTURE\r\n\t\t * De la même taille que la Box du texte\r\n\t\t */\r\n\t\tconst renderTexture = PIXI.RenderTexture.create(text.width, text.height)\r\n\t\tconst renderTextureSprite = new PIXI.Sprite(renderTexture)\r\n\t\t// Masking\r\n\t\ttext.mask = renderTextureSprite\r\n\t\t// ajout des enfants\r\n\t\tthis.textGroup.addChild(text)\r\n\t\tthis.textGroup.addChild(renderTextureSprite)\r\n\t\tconst _w = window.innerWidth\r\n\t\tconst _h = window.innerHeight\r\n\t\tthis.textGroup.x = _w / 2 - text.width / 2\r\n\t\tthis.textGroup.y = _h / 2 - text.height / 2\r\n\r\n\t\tslideContainer.addChild(this.textGroup)\r\n\t\t// ajout sur la scène globale\r\n\t\tstage.addChild(slideContainer)\r\n\r\n\t\t/**\r\n\t\t *  On remplit de blanc le render Texture,\r\n\t\t *  ce qui nous permet d'afficher entierement\r\n\t\t *  les calques masqué par celui-ci\r\n\t\t * */\r\n\t\tconst blackBoard = new PIXI.Graphics()\r\n\t\tblackBoard.beginFill(0xffffff)\r\n\t\tblackBoard.drawRect(0, 0, _w, _h)\r\n\t\tblackBoard.endFill()\r\n\t\trenderer.render(blackBoard, renderTexture, false, null, false)\r\n\r\n\t\t// accessible depuis l'exterieur\r\n\t\tthis.renderTexture = renderTexture\r\n\t\tthis.slideContainer = slideContainer\r\n\t}\r\n}\r\n\r\nexport default TextSlide\r\n"},"lineCount":null}},"error":null,"hash":"d375972d7b9b0e8d5e6f8be1dd580f6b","cacheData":{"env":{}}}