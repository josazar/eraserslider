n  &::before {\r\n    content: '';\r\n    position: absolute;\r\n    height: 2px;\r\n    width: 00%;\r\n    bottom: 10px;\r\n    left: 0;\r\n    background-color: $beige;\r\n    left: 0;\r\n    right: 0;\r\n    margin: 0 auto;\r\n    transition: all 0.4s;\r\n  }\r\n\r\n  &:hover {\r\n    &::before {\r\n      width: 100%;\r\n    }\r\n  }\r\n}\r\n\r\n// ALIGN\r\n.align-center {\r\n  margin: 0 auto;\r\n  display: block;\r\n  text-align: center;\r\n  width: max-content;\r\n  left: 0;\r\n  right: 0;\r\n}\r\n\r\n.cta-slider {\r\n  position: absolute;\r\n  top: 40vh;\r\n}\r\n","src\\styles\\colors.scss":"$beige: #DEC2BE;\r\n$bluedark: #1A252C;\r\n"},"lineCount":null}},"error":null,"hash":"bb816f2812d8e9a5371b03a0ba5dca27","cacheData":{"env":{}}}